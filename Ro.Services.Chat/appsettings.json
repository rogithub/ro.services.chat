{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "Serilog": {
        "Using": [
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "logs/log.txt"
                }
            }
        ],
        "Properties": {
            "Application": "Ro.Services.Chat",
            "Environment": "Production"
        }
    },
    "App": {
        "Groups": [
            "Canadá",
            "Estados Unidos",
            "México",
            "Guatemala",
            "Belice",
            "El Salvador",
            "Honduras",
            "Nicaragua",
            "Costa Rica",
            "Panamá",
            "Colombia",
            "Venezuela",
            "Ecuador",
            "Peru",
            "Bolivia",
            "Paraguay",
            "Chile",
            "Argentina",
            "Cuba",
            "República Dominicana",
            "Puerto Rico",
            "España"
        ]
    }
}